// Motive of this code is to find the numbers which are not arranged in a systematic manner and arrange them and till the last iteration to find the number which is arranged according to the code 
int findFirstMissingPositive(int[] nums)
{
    for(int i = 1 ; i <= nums.length ; i++)
    {
        while (nums[i-1] != i)
        {
            int temp = nums[i-1];
            if (temp-1 < 0 ||
                temp-1 >= nums.length ||
                nums[temp-1] == temp)
            {
                break;
            }

            nums[i-1] = nums[temp-1];
            nums[temp-1] = temp;
        }
    }

    for(int i = 1 ; i <= nums.length ; i++)
    {
        if (nums[i-1] != i) return i;
    }

    return nums.length;
}
